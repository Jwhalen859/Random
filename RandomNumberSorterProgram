<html>
<head><title>Odd Even Randoms</title>
<script>
    var even = new Array();
    var odd = new Array();
    for (i=0; i<25; ++i) {
        var r = Math.floor(Math.random() * 100) + 1;
        if (r%2==0) even.push(r);
        else odd.push(r);
    }
    document.write("Even numbers were: ");
    for (i=0; i < even.length; ++i)
       document.write(even[i] + " ");
       
    document.write("<br>");
    
    document.write("Odd numbers were: ");
    for (i=0; i < odd.length; ++i)
       document.write(odd[i] + " ");
</script>
</head>
<body>
</body>
</html>
